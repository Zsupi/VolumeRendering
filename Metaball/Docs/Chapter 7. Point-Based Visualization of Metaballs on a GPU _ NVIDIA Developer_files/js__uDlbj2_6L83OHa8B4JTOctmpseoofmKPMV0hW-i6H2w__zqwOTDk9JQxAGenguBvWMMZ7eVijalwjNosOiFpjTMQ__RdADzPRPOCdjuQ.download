(() => {
  const replaceTag = (id) => {
    const that = document.getElementById(id)
    if (!that) {
      console.debug(`Skip element replacement ${id}`, )
      return
    }
    const p = document.createElement('p')

    p.setAttribute('id', that.getAttribute('id'))

    while (that.firstChild) {
      p.appendChild(that.firstChild)
    }

    that.parentNode.replaceChild(p, that)
  }

  const alterOneTrustDom = () => {
    const ids = [
      'pc-title',
      'manage-cookies-text',
      'ot-header-id-C0001',
      'ot-header-id-C0002',
      'ot-header-id-C0003',
      'vendors-list-title'
    ]

    setTimeout(() => ids.forEach((i) => replaceTag(i)), 2000)
  }

  document.addEventListener('DOMContentLoaded', alterOneTrustDom)
})()



